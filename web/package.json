{
  "name": "@simplepdf/web-embed-pdf",
  "version": "1.8.2",
  "description": "SimplePDF straight into your website",
  "repository": {
    "type": "git",
    "url": "https://github.com/bendersej/simplepdf-embed/tree/main/web"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "main": "dist/index.js",
  "browser": "dist/index.min.js",
  "unpkg": "dist/index.min.js",
  "types": "dist/index.d.ts",
  "author": "bendersej",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test:types": "tsc --noEmit",
    "test:format": "npm run prettier -- --check",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "prettier": "prettier .",
    "format": "npm run prettier -- --write",
    "prepublishOnly": "rimraf dist && npm run build",
    "build": "rollup -c",
    "start": "rollup -c -w"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jsdom": "^21.1.7",
    "jsdom": "^26.0.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.30.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.2"
  },
  "files": ["src", "dist"],
  "keywords": ["web", "typescript", "npm", "pdf"]
}
